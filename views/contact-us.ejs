                height="450"
                style="border: 0"
                allowfullscreen=""
                loading="lazy"
              ></iframe>
            </div>
            <div class="contact-right">
              <div class="contact-head">
                <h3 style="text-transform: uppercase">contact us</h3>
                <p class="text text-md" style="text-transform: capitalize">
                  elhayat group is always ready to respond to all proposals that
                  interest you regarding home service, through service employees
                  at all working hours..! please register your suggestions in
                  the message box, and they will be shown to the specialist.
                </p>
              </div>
              <form id="contactForm">
                <div class="form-element">
                  <input
                    required
                    type="text"
                    class="form-control"
                    placeholder="your name"
                    id="name"
                    name="name"
                  />
                </div>
                <div class="form-element">
                  <input
                    required
                    type="email"
                    class="form-control"
                    placeholder="your email"
                    id="email"
                    name="email"
                  />
                </div>
                <div class="form-element">
                  <input
                    required
                    type="tel"
                    class="form-control"
                    placeholder="your email"
                    id="phone"
                    name="phone"
                  />
                </div>
                <div class="form-element">
                  <textarea
                    required
                    rows="5"
                    placeholder="your message"
                    class="form-control"
                    id="message"
                    name="message"
                  ></textarea>
                </div>
                <button type="submit" class="btn btn-white btn-submit">
                  <i class="fas fa-arrow-right"></i> send message
                </button>
              </form>
            </div>
          </div>
        </section>
      </main>
      <%- include("./navandfooter/footer.ejs") %>
    </div>
    <%- include("./navandfooter/scripts.ejs") %>
    <script>
      const booking = document.getElementById("contactForm");
      booking.addEventListener("submit", function (event) {
        event.preventDefault(); // Prevent the form from submitting normally

        const name = booking.querySelector("#name").value;
        const phone = booking.querySelector("#phone").value;
        const email = booking.querySelector("#email").value;
        const message = booking.querySelector("#message").value;

        const form = {
          name: name,
          phone: phone,
          email: email,
          message: message,
        };

        fetch("/contact", {
          method: "POST",
          body: JSON.stringify(form),
          headers: {
            "Content-Type": "application/json",
          },
        })
          .then((response) => response.json())
          .then((data) => {
            if (data.success) {
              alert("Message Has Been sent successfully!");
            } else {
              alert("contact failed. Please try again.");
            }
          })
          .catch((error) => {
            console.error("Error:", error);
            alert("An error occurred. Please try again.");
          });
      });
    </script>
